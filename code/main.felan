byte :: i8;
ubyte :: u8;

short :: i16;
ushort :: u16;

int :: i32;
uint :: u32;

long :: i64;
ulong :: u64;

// short_float :: f16;
float :: f32;
double :: f64;
long_double :: f128;

usize :: u64;


main :: () -> void {
  print_u64(1234);
};

print_u64 :: (value:u64) -> void {
  value := value;
  digit : u64 = undefined;
  first_time := true;
  while {
    first_time = false;
    digit = value % 10;
    putc 'a';
    value /= 10;
    value != 0;
  } {}
};
