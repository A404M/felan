main :: () -> void {
  a : [10]u8 = undefined;
  a[0] = 2;
  print(123);
};

print :: (value:i64)->void{
  value := value;
  a : [20]u8 = undefined;
  i := 0;
  while {
    a[i] = '0' + @cast(value % 10,u8);
    i += 1;
    value /= 10;
    value != 0;
  } {}

  while i > 0 {
    putc a[i-1];
    i -= 1;
  }
};
