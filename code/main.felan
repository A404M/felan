@import("basic.felan");

print :: (comptime t:type,v:t)->void{
  if @typeOf(v) == u8
    putc v;
};

main :: () -> void {
  i := 0;
  while i < 10 {
    i += 1;
    if i == 7
      return;
    else if i % 2 == 0
      continue;
    else if i == 8
      break;
    print(u8,@cast(i,u8)+'0');
    print(u16,@cast(i,u16));
  }
};
